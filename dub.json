{
	"name": "scriptlike",
	"description": "Utility to aid in script-like programs.",
	"authors": ["Nick Sabalausky", "Jesse Phillips"],
	"homepage": "https://github.com/abscissa/scriptlike",
	"license": "zlib/libpng",

	"excludedSourceFiles": ["src/scriptlike/packageVersion.d"],
	
	"configurations": [
		{
			"name": "library",
			"targetType": "library",
			"targetPath": "bin",
			"importPaths": ["src"]
		},
		{
			"name": "no-build",
			"targetType": "library",
			"targetPath": "deleteme",
			"importPaths": ["dummy"],
			"sourcePaths": ["dummy"],
			"excludedSourceFiles": ["src/*"]
		}
	]
}
